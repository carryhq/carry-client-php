{
  "name": "carryhq/carry-client-php",
  "require": {
    "php": "^7.4",
    "ext-curl": "*",
    "ext-json": "*",
    "guzzlehttp/guzzle": "^7.2",
    "guzzlehttp/psr7": "^1.7",
    "psr/http-client": "^1.0",
    "psr/http-message": "^1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.0"
  },
  "autoload": {
    "psr-4": {
      "Carry\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "CarryTest\\": "test/"
    }
  },
  "config": {
    "sort-packages": true
  }
}
